    "nette/security": "^3.1",
    "nette/utils": "^3.2",
    "latte/latte": "^2.9",
    "tracy/tracy": "^2.8",
    "contributte/console": "^0.9.1",
    "nettrine/orm": "^0.8.1",
    "nettrine/dbal": "^0.7.0",
    "guzzlehttp/guzzle": "^6.5",
    "composer/ca-bundle": "^1.2",
